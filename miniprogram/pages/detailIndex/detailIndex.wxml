<view class="list">
    <view wx:for="{{list}}" wx:key="title" class="view-item">
      <view class="img">
        <image src="{{item.cover}}" />
      </view>
      <view class="detail">
        <view class="detail-title">
          <text>{{item.title}}</text>
        </view>
        <view>
          <text class="score-wrapper">{{item.score}}</text>
          <text class="score">{{item.rate}}</text>
        </view>
      </view>
    </view>
  </view>